{
    "version": 2,
    "builds": [
      {
        "src": "api/recettes.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/recettes.js"
      }
    ]
  }
  